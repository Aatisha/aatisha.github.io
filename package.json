{
  "name": "web-portfolio",
  "version": "1.0.0",
  "description": "A personal web portfolio",
  "author": "Aatisha Cyrill",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.js",
    "start:docs": "npm run lint-fix && cross-env NODE_ENV=production webpack-dev-server  --max-old-space-size=8192 --config ./config/webpack.config.js && http-server ./docs -o",
    "setup": "node ./config/site.setup.js",
    "lint-fix": "npm run lint-fix:styles && npm run lint-fix:js",
    "lint:js": "./node_modules/.bin/eslint \"src/**/*.js\"",
    "lint-fix:js": "npx eslint \"src/**/*.js\" --fix",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint-fix:styles": "npx stylelint \"src/**/*.scss\" --fix",
    "build:docs": "cross-env NODE_ENV=production webpack --max-old-space-size=8192 --config ./config/webpack.config.js",
    "deploy": "node ./config/site.deploy.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "favicons-webpack-plugin": "^4.2.0",
    "file-loader": "^4.2.0",
    "glob": "^7.1.4",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "http-server": "^0.11.1",
    "image-webpack-loader": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-sass": "^0.4.1",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "robotstxt-webpack-plugin": "^6.0.0",
    "sass-loader": "^7.2.0",
    "sitemap-webpack-plugin": "^0.6.0",
    "style-loader": "^1.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.9.3",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1",
    "webpackbar": "^4.0.0"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^25.0.0",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "enquirer": "^2.3.6",
    "figlet": "^1.2.3",
    "ftp-deploy": "^2.4.1",
    "jquery": "^3.6.0",
    "layout-bmfont-text": "^1.3.4",
    "lightgallery": "^2.7.0",
    "normalize.css": "^8.0.1",
    "quad-indices": "^2.0.1",
    "reset-css": "^5.0.1",
    "sanitize.css": "^12.0.0",
    "simplex-noise": "^4.0.3",
    "swiper": "^11.1.15",
    "three": "^0.155.0",
    "vanilla-tilt": "^1.8.1",
    "webpack-dev-server": "^3.11.2"
  }
